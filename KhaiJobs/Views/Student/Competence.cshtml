@{Layout = "~/Views/Shared/_Student.cshtml";}
@{
    ViewBag.Title = "Competence";
}
<title>@ViewBag.Title</title>
<div class="container content-main">
    <div class="row">
        <div class="col-lg-12">

            <h3 class="profile">Компетенции</h3>
            <hr style="border-top:5px solid #ccc;" />
            <h4>Добавить компетенцию</h4>

            <form>
                <div class="form-group">
                    <label from="nameComp">Название компетенции</label>
                    <input type="text" class="form-control" id="nameComp" name="nameComp" />
                </div>
                <div class="form-group">
                    <label from="typeComp">Тип компетенции</label>
                    <select class="form-control " id="typeComp" name="typeComp"></select>
                </div>
                <div class="form-group">
                    <label from="type2Comp">Тип2 компетенции</label>
                    <select class="form-control " id="type2Comp" name="type2Comp"></select>
                </div>



                <div>
                    <button class="btn btn-primary">Добавить</button>
                </div>
            </form>
            <hr style="border-top:5px solid #ccc;" />
            <label from="typeComp">Выберете компетенцию</label>
            <select class="form-control " id="typeComp" name="typeComp"></select>
            <hr style="border-top:1px solid #ccc;" />
            <div>
                <h4>Вывод компетенции</h4>

                <!--
                    Данные о компетенции
                -->
            </div>
        </div>
    </div>
</div>

<link href="~/Content/bootstrap.min.css" rel="stylesheet" media="screen">
<link href="~/Content/jobs.css" rel="stylesheet" />
